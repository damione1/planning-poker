package templates

templ VotingCards(values []string) {
	<div class="voting-cards" x-data="cardSelector()">
		for _, value := range values {
			<button
				class="card"
				data-value={ value }
				@click="selectCard($el.dataset.value)"
				:class="{ 'selected': selected === $el.dataset.value }">
				{ value }
			</button>
		}
	</div>
}
